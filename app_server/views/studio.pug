extends layout

include include/smix

block content

	.studio-list

		.studio-nav
			include ./include/sadd-menu

		if studios	
			ul
				for studio in studios
					.studio-list-item.mr-4
						li
							a(href ='/studio/' + studio.url)
							for cover_image in studio.cover_image
								+fixDir(cover_image.path)

							p
								a(href ='/studio/' + studio.url) #{studio.name}

		else
			p There are no studios available